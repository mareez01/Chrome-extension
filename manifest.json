{
    "manifest_version": 3,
    "name": "LinkedIn Inspector",
    "version": "1.0",
    "description": "Inspect LinkedIn pages and store data in Google Sheets",
    "permissions": [
      "activeTab",
      "storage",
      "webNavigation",
      "https://*.linkedin.com/",
      "https://script.google.com/",
      "identity"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    },
    "devtools_page": "devtools.html"
  }
  